<mujoco model="two_dof_robot">
  <compiler angle="radian" meshdir="assets/"/>
  <worldbody>
    <body name="upperarm">
      <inertial pos="0.5 0 0" quat="0.5 0.5 0.5 0.5" mass="27" diaginertia="0.225 0.225 0.0225"/>
      <joint name="joint1" pos="0 0 0" axis="0 0 1" range="-3.14 3.14" actuatorfrcrange="-1000 1000" frictionloss="1.0" damping="2.0"/>
      <body name="forearm" pos="1 0 0" gravcomp="1.0">
        <inertial pos="0.5 0 0" quat="0.5 0.5 0.5 0.5" mass="27" diaginertia="0.225 0.225 0.0225"/>
        <joint name="joint2" pos="0 0 0" axis="0 0 1" range="-3.14 3.14" actuatorfrcrange="-1000 1000"/>
        <geom size="0.5 0.05 0.05" pos="0.5 0 0" type="box" class="collision"/>
        <geom size="0.5 0.05 0.05" pos="0.5 0 0" type="box" rgba="0 0 1 1" class="visual"/>
        <geom size="0.05 0.05 0.05" pos="1.05 0 0.1" type="box" class="collision"/>
        <geom size="0.05 0.05 0.05" pos="1.05 0 0.1" type="box" rgba="0.2 0.2 0.2 1" class="visual"/>
        <site name="forearm" pos="0 0 0" quat="1.0 0 0 0"/>
        <site name="ee_link" pos="1.0 0 0" quat="1.0 0 0 0"/>
        <site name="camera_link" pos="1.05 0 0.1" quat="1.0 0 0 0"/>
        <site name="camera_color_frame" pos="1.0 0 0.1" quat="1.0 0 0 0"/>
        <site name="camera_color_optical_frame" pos="1.0 0 0.1" quat="-0.49999816339744824 0.4999999999966268 0.5000018366025517 -0.4999999999966268"/>
        <site name="camera_color_mujoco_frame" pos="1.0 0 0.1" quat="0.5000006633911983 0.4999974999995599 0.49999933659630164 0.5000024999995599"/>
        <camera name="camera" fovy="58" mode="fixed" resolution="640 480" pos="1.0 0 0.1" quat="-0.4999999999966268 -0.49999816339744824 -0.4999999999966268 -0.5000018366025517"/>
      </body>
      <geom size="0.5 0.05 0.05" pos="0.5 0 0" type="box" class="collision"/>
      <geom size="0.5 0.05 0.05" pos="0.5 0 0" type="box" rgba="1 0 0 1" class="visual"/>
      <site name="upperarm" pos="0 0 0" quat="1.0 0 0 0"/>
    </body>
    <site name="world" pos="0 0 0" quat="1.0 0 0 0"/>
    <site name="base" pos="0 0 0" quat="1.0 0 0 0"/>
    <site name="lidar_sensor_frame" pos="1.0 2.0 0" quat="0.7071054825112363 0 0 -0.7071080798594737"/>
    <body name="lidar_sensor_frame_lidar_body" pos="1.0 2.0 0" quat="0.6991665401770936 0.6991665401770936 -0.10567000093116563 -0.10567000093116563">
      <replicate site="lidar_sensor_frame" count="24" sep="-" offset="0 0 0" euler="0 0.025 0">
        <site name="rf" pos="0.0 0.0 0.0" quat="0.0 0.0 0.0 1.0"/>
      </replicate>
    </body>
  </worldbody>
  <option integrator="implicitfast"/>
  <default>
    <default class="visual">
      <geom group="2" type="mesh" contype="0" conaffinity="0"/>
    </default>
    <default class="collision">
      <geom group="3" type="mesh"/>
    </default>
  </default>
  <actuator>
    <position name="joint1" joint="joint1" kp="1000" dampratio="1.0"/>
    <position name="joint2" joint="joint2" kp="1000" dampratio="1.0"/>
    <!-- <motor name="joint1_motor" joint="joint1" gear="1"/>
    <motor name="joint2_motor" joint="joint2" gear="1"/> -->
    <!-- <velocity name="joint1_vel" joint="joint1" kv="50"/>
    <velocity name="joint2_vel" joint="joint2" kv="30"/> -->

  </actuator>
  <sensor>
    <rangefinder name="lidar" site="rf"/>
  </sensor>
</mujoco>
